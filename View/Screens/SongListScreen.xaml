<ResourceDictionary
    x:Class="View.Screens.SongListScreen"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <DataTemplate x:Key="SongListScreen">
        <!-- Start songlist screen -->
        <Grid Margin="0,0,0,60">
            <ContentPresenter ContentTemplate="{DynamicResource SongList}" />
        </Grid>
    </DataTemplate>
</ResourceDictionary>