<Window x:Class="View.PlaylistMenu"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:View"
        mc:Ignorable="d"
        Title="PlaylistMenu" Height="450" Width="800">
    <Grid>
        <Button Content="Home" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Click="Button1_Click"/>
        <Label x:Name="Titel" Content="Name playlist" HorizontalAlignment="Left" Margin="85,42,0,0" VerticalAlignment="Top" Width="170"/>
        <ListBox x:Name="playlistList" RenderTransformOrigin="0.17,0.491" Margin="85,73,118,31"/>
        <Button Content="Playlists" HorizontalAlignment="Left" Margin="10,42,0,0" VerticalAlignment="Top" Click="Button2_Click" RenderTransformOrigin="0.597,2.259"/>

    </Grid>
</Window>
